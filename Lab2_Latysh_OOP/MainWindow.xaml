            <Button Content="4" HorizontalAlignment="Left" Height="66" Margin="0,382,0,0" VerticalAlignment="Top" Width="100"/>
            <Button Content="5" HorizontalAlignment="Left" Height="66" Margin="100,382,0,0" VerticalAlignment="Top" Width="100"/>
            <Button Content="6" HorizontalAlignment="Left" Height="66" Margin="200,382,0,0" VerticalAlignment="Top" Width="100"/>
            <Button Content="-" HorizontalAlignment="Left" Height="66" Margin="300,382,0,0" VerticalAlignment="Top" Width="100"/>
            <Button Content="7" HorizontalAlignment="Left" Height="66" Margin="0,311,0,0" VerticalAlignment="Top" Width="100"/>
