            <Button Content="1" HorizontalAlignment="Left" Height="66" Margin="0,453,0,0" VerticalAlignment="Top" Width="100"/>
            <Button Content="2" HorizontalAlignment="Left" Height="66" Margin="100,453,0,0" VerticalAlignment="Top" Width="100"/>
            <Button Content="3" HorizontalAlignment="Left" Height="66" Margin="200,453,0,0" VerticalAlignment="Top" Width="100"/>
