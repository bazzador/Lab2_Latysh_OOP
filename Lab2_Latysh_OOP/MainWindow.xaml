            <Button Content="9" HorizontalAlignment="Left" Height="66" Margin="200,311,0,0" VerticalAlignment="Top" Width="100"/>
            <Button Content="×" HorizontalAlignment="Left" Height="66" Margin="300,311,0,0" VerticalAlignment="Top" Width="100"/>
            <Button Content="CE" HorizontalAlignment="Left" Height="66" Margin="0,240,0,0" VerticalAlignment="Top" Width="100"/>
            <Button Content="C" HorizontalAlignment="Left" Height="66" Margin="100,240,0,0" VerticalAlignment="Top" Width="100"/>
            <Button Content="⌫" HorizontalAlignment="Left" Height="66" Margin="200,240,0,0" VerticalAlignment="Top" Width="100"/>
